{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon_48.png",
      "default_title": "NTU Full Text",
      "name": "NTU Full Text Plugin"
   },
   "description": "A plugin allows NTU staffs and students to seamlessly retrieve full text.",
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png",
      "64": "icon_64.png"
   },
   "manifest_version": 2,
   "name": "NTU Full Text",
   "permissions": [ "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.0"
}
